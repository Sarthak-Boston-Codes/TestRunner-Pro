{
  "name": "testrunner-pro",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node src/app.js",
    "start:dev": "nodemon src/app.js",
    "build:client": "webpack --mode production",
    "dev:client": "webpack serve --mode development --open",
    "dev": "concurrently \"npm run start:dev\" \"npm run dev:client\"",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "cross-env NODE_ENV=test jest tests/integration",
    "test:e2e": "cross-env NODE_ENV=test jest tests/e2e/selenium --runInBand --testTimeout=30000",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "lint": "eslint . --ext .js,.jsx",
    "db:migrate": "node scripts/migrate.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@faker-js/faker": "^10.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "chromedriver": "^131.0.0",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^30.2.0",
    "jest-cli": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "selenium-webdriver": "^4.38.0",
    "style-loader": "^4.0.0",
    "supertest": "^7.1.4",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
